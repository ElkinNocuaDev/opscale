{
    "name": "horaceho/fullcalendar",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "maddhatter/laravel-fullcalendar": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Horaceho\\Fullcalendar\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Horaceho\\Fullcalendar\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
